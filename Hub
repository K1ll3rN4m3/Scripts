Tab1:Label('Scripts Sections')
Tab2:Label('Gui Section')

-- scripts function:

Tab1:Button('Disco', function()
while true do

		game.Lighting.Ambient = Color3.new(math.random(), math.random(), math.random())
		wait(.25)

	end
end)

