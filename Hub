_G.Settings = {
['Name'] = 'DarkAngel Hub',
['Intro'] = true,
['Keybind'] = 'G'
}

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/QPehPJ6m", true))()

local Tab1 = Library:CreateTab('Scripts')
